PLOT_OPTS=""
FLAG_POWER_TEST=0
FLAG_THROUGHPUT_TEST=0
while getopts "23i:n:o:s:z" OPT; do
	case ${OPT} in
	2)
		FLAG_POWER_TEST=1
		PLOT_OPTS="${PLOT_OPTS} -2"
		;;
	3)
		FLAG_THROUGHPUT_TEST=1
		PLOT_OPTS="${PLOT_OPTS} -3"
		;;
	i)
		INDIR=$OPTARG
		QTIME=$INDIR/q_time.csv
		;;
	n)
		STREAMS=${OPTARG}
		;;
	o)
		OUTFILE=${OPTARG}
		;;
	s)
		SCALE_FACTOR=${OPTARG}
		;;
	z)
		PLOT_OPTS="${PLOT_OPTS} -z"
		;;
	\?)
		exit 1
		;;
	esac
done

which R > /dev/null 2>&1
if [ $? -ne 0 ]; then
	exit 1
fi

